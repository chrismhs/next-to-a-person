{
  "name": "Next to a person",
  "version": "1.1",
  "description": "Get your product next to a person",
  "manifest_version": 2,
  "permissions": ["tabs"],
  "background": {
    "scripts": ["extension.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "images/icon-32x32.png"
  },
  "icons": {
    "16": "images/icon-16x16.png",
    "32": "images/icon-32x32.png",
    "64": "images/icon-64x64.png",
    "128": "images/icon-128x128.png"
  }
}
